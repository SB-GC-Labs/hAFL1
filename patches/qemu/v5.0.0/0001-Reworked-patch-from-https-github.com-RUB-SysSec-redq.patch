+static uint64_t limit_to_word_width(redqueen_t* self, uint64_t val){
+	switch(self->cpu->disassembler_word_width){
+	case 64:
+		return val;
+	case 32: 
+		return val & 0xffffffff;
+	default:
+		assert(false);
+	}
+}
+
+static uint64_t word_width_to_bytes(redqueen_t* self){
+	switch(self->cpu->disassembler_word_width){
+	case 64:
+		return 8;
+	case 32: 
+		return 4;
+	default:
+		assert(false);
+	}
+}
