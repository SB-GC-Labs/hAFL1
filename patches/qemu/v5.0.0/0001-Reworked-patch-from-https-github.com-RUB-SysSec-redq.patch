+static bool is_trace_entry_point(redqueen_t* self, uint64_t addr){
+	if(addr >= self->address_range_start && addr <= self->address_range_end){
+    return self->bitmap[addr-self->address_range_start] & CMP_BITMAP_TRACE_ENABLED;
+  }
+  return false;
+}
