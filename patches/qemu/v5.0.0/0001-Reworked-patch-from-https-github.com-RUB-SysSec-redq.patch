+    _redqueen_update_whitelist(self);
+    _redqueen_update_blacklist(self);
+		redqueen_insert_hooks(self);
+		self->intercept_mode = true;
+		//((uint8_t*) payload_buffer)[PAYLOAD_SIZE-1] = 1;
+	}
