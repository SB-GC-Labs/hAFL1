+typedef struct kafl_mem_state {
+	DeviceState parent_obj;
+
+	Chardev *kafl_chr_drv_state;
+	CharBackend chr;
+	
+	char* redqueen_workdir;
+	char* data_bar_fd_0;
+	char* data_bar_fd_1;
+	char* data_bar_fd_2;
